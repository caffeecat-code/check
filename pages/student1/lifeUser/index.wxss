.mainBtn{
    margin:0 auto;
    font-size: 30rpx;
    color: #fff;
    margin-top: 50rpx;
    /* background-image: -moz-linear-gradient( -178deg, rgb(66,229,123) 0%, rgb(66,233,125) 25%, rgb(55,249,214) 100%);
    background-image: -webkit-linear-gradient( -178deg, rgb(66,229,123) 0%, rgb(66,233,125) 25%, rgb(55,249,214) 100%);
    background-image: -ms-linear-gradient( -178deg, rgb(66,229,123) 0%, rgb(66,233,125) 25%, rgb(55,249,214) 100%);
   */
    width: 100rpx; 
    background-image: linear-gradient( 135deg, #ABDCFF 10%, #0396FF 100%);
    height: 50rpx;
    text-align: center;
    line-height:50rpx;
    border-radius: 10rpx;
}
.addUser{
    font-size: 30rpx;
    color: #fff;
    /* background-image: -moz-linear-gradient( -178deg, rgb(66,229,123) 0%, rgb(66,233,125) 25%, rgb(55,249,214) 100%);
    background-image: -webkit-linear-gradient( -178deg, rgb(66,229,123) 0%, rgb(66,233,125) 25%, rgb(55,249,214) 100%);
    background-image: -ms-linear-gradient( -178deg, rgb(66,229,123) 0%, rgb(66,233,125) 25%, rgb(55,249,214) 100%);
   */
    width: 20%; 
    background-image: linear-gradient( 135deg, #87CEEB 10%, #0396FF 100%);
    border-radius: 10rpx;
    height: 70rpx;
    text-align: center;
    line-height: 70rpx;
    margin-bottom: 10rpx;
}
.wsui-btn__hover_btn {
    opacity: 0.9;
    transform: scale(0.95, 0.95);
    position: relative;
    top: 3rpx;
    left: 3rpx;
    box-shadow:0px 0px 8px rgba(0, 0, 0, .1) inset; 
}